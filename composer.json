{
    "name": "de_zunke/archery-tournament",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
	"autoload-dev": {
		"psr-4": {
			"App\\Tests\\": "tests/"
		}
	},
    "authors": [
        {
            "name": "Denis Zunke",
            "email": "denis.zunke@gmail.com"
        }
    ],
    "require": {
        "doctrine/dbal": "^4.4",
        "doctrine/doctrine-bundle": "^3.2",
        "doctrine/migrations": "^3.9",
        "symfony/asset-mapper": "^8.0",
        "symfony/console": "^8.0",
        "symfony/debug-bundle": "^8.0",
        "symfony/dotenv": "^8.0",
        "symfony/form": "^8.0",
        "symfony/framework-bundle": "^8.0",
        "symfony/http-client": "*",
        "symfony/mime": "^8.0",
        "symfony/password-hasher": "^8.0",
        "symfony/runtime": "*",
        "symfony/security-bundle": "^8.0",
        "symfony/security-csrf": "^8.0",
        "symfony/twig-bundle": "^8.0",
        "symfony/uid": "^8.0",
        "symfony/ux-icons": "^2.32",
        "symfony/yaml": "^8.0",
        "webmozart/assert": "^2.1"
    },
	"config": {
		"sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "symfony/runtime": true
        }
	},
    "require-dev": {
        "doctrine/coding-standard": "^14.0",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-phpunit": "^2.0",
        "phpstan/phpstan-strict-rules": "^2.0",
        "phpstan/phpstan-webmozart-assert": "^2.0",
        "phpunit/phpunit": "^12.4",
        "rector/rector": "^2.2",
        "spaze/phpstan-disallowed-calls": "^4.7"
    },
    "scripts": {
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "php bin/console cache:clear",
            "assets:install %PUBLIC_DIR%": "php bin/console assets:install public"
        }
    }
}
